// A display shows a fake company name, and if it is below the required temperature.
// TMP sensor checks how cold the room is.
// If the room is cold enough; a servo motor will be put in the "locked" position and the screen will be the "goodTemperature" bitmap.
// If the room is too hot; the servo motor will move to the "unlocked" position, a piezo buzzer will sound, a LED will be flashing red,
// and the screen will be the "tooHot" bitmap.

#include <Servo.h>
#include <U8g2lib.h>
#include "Wire.h"

U8G2_SH1106_128X64_NONAME_2_HW_I2C display(U8G2_R0, /* reset=*/U8X8_PIN_NONE);

extern const unsigned char welcomeBitmap[] U8X8_PROGMEM;
extern const unsigned char goodTemperatureBitmap[] U8X8_PROGMEM;
extern const unsigned char tooHotBitmap[] U8X8_PROGMEM;

const int ledRed = 13;
const int buzzerPin = 12;
const int servoPin = 7;
const int tmpPin = A0;

Servo doorServo;

void setup() {
  pinMode(ledRed, OUTPUT);
  pinMode(buzzerPin, OUTPUT);

  doorServo.attach(servoPin);
  doorServo.write(90); // Start in locked position
  Serial.begin(9600);
	display.begin();
	display.firstPage();
	do {
  display.drawXBMP(0, 0, 128, 64, welcomeBitmap);
	} while (display.nextPage());
	delay(5000);
}


void loop() {
  int reading = analogRead(tmpPin);
  int tmpPinValue = analogRead(tmpPin);
  
  //get the voltage
  float voltage = tmpPinValue * (5.0 / 1023.0);
  
  //convert to °C
  float temperatureC = (voltage - 0.5) * 100.0;
  
  //convert to °F
  float temperatureF = (temperatureC * 9.0/5.0) + 32;

  int temp = (int)temperatureF;
  Serial.print("Temperature (F): ");
  Serial.println(temperatureF);

  display.firstPage();
  do {
    if(temperatureF >= 70.0){
      display.drawXBMP(0, 0, 128, 64, tooHotBitmap);
      badTemperature();
    }
  
    else {
      display.drawXBMP(0, 0, 128, 64, goodTemperatureBitmap);
      goodTemperature();
    }
  }  while (display.nextPage());

  delay(2000);

}

void goodTemperature() {
  digitalWrite(ledRed, LOW);
  noTone(buzzerPin);
  doorServo.write(90);
}

void badTemperature() {
  digitalWrite(ledRed, HIGH);
  delay(500);
  digitalWrite(ledRed, LOW);
  alertTooHot();
  doorServo.write(0);
}

void alertTooHot() {
  tone(buzzerPin, 300, 300);  // Low buzz
  delay(350);
  tone(buzzerPin, 988, 200);  // High buzz
  delay(350);
}

